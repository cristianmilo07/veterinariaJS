let miArray2 = []
let control = 0;
while(control < 10) {
  miArray2.push(control)
  if (control % 5 === 0) {
    control = control + 2
  } else {
    control = control + 1
  }
}


miArray2


let miArray = ['hola', 'buen día', 'adios'];
function convertirMayusculas(actual, indice, arrayCompleto) {
  const respuesta = actual.toUpperCase()
  console.log(respuesta)
  console.log(indice)
  console.log(arrayCompleto)
}

let miVar = miArray.forEach(convertirMayusculas)
miVar


let miArrays = ['hola', 'buen día', 'adios'];
function convertirMayusculass(actual, indice, arrayCompleto) {
  const respuesta = actual.toUpperCase()
  return respuesta
}

let miVarr = miArrays.map(convertirMayusculass)
miVarr


function sumar(num1, num2){
  return num1+ num2
}

function restar(num1, num2){
  return num1 - num2
}

function multiplicar(num1, num2){
  return num1 * num2
}

function multiFuncion(num1, num2, operacion) {
  const resultado = operacion(num1, num2);
  console.log(resultado)
}
multiFuncion(5,4, sumar)
multiFuncion(5,4, multiplicar)
multiFuncion(5,4, restar)

